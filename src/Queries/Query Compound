# Query para obter o APY diário (Tentativa 2)
query GetMarketTimeSeries_Attempt2 {
  marketDailySnapshots(
    orderBy: timestamp
    orderDirection: asc
    first: 1000 
    skip: 1177 # Alterar aqui para pular quantidade de dados 
    
    where: {
      market: "0xc3d688b66703497daa19211eedff47f25384cdc3a0b86991c6218b36c1d19d4a2e9eb0ce3606eb48"
    }
  ) {
    id
    timestamp
    
    # --- HIPÓTESE 1: APY está em 'rates' ---
    # Vamos focar aqui, já que 'depositAPY' falhou.
    rates {
      id
      rate  # Esta deve ser a taxa (em %)
      side  # "SUPPLY" (Depósito) ou "BORROW" (Empréstimo)
    }
    
    # --- HIPÓTESE 2: Encontrar a Utilização  ---
    # Já que 'utilizationRate' falhou, vamos tentar 
    # os totais para calcular manualmente.
    # Nomes prováveis:
    totalDepositBalanceUSD
    totalBorrowBalanceUSD
    
    # Outros campos que podem ser úteis para o seu PDF [cite: 11]
    inputTokenPriceUSD 
  }
} # usada no The Graph https://thegraph.com/explorer/subgraphs/AwoxEZbiWLvv6e3QdvdMZw4WDURdGbvPfHmZRc8Dpfz9?view=Query&chain=arbitrum-one
